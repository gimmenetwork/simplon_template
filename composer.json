{
  "name": "gimmenetwork/simplon_template",
  "description": "[FORK] Simple templating solution w/ assets handling (mustache, phtml, assets)",
  "homepage": "https://github.com/fightbulc/simplon_template",
  "type": "library",
  "keywords": [
    "templates",
    "mustache",
    "phtml",
    "fast",
    "lightweight"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Tino Ehrich",
      "email": "tino@bigpun.me",
      "role": "developer"
    }
  ],
  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/gimmenetwork/simplon_mustache"
    },
    {
      "type": "vcs",
      "url": "https://github.com/gimmenetwork/simplon_phtml"
    }
  ],
  "require": {
    "php": ">=5.4",
    "gimmenetwork/simplon_mustache": "0.1.*",
    "gimmenetwork/simplon_phtml": "^0.2"
  },
  "autoload": {
    "psr-4": {
      "Simplon\\Template\\": "src/"
    }
  },
  "replace": {
    "simplon/template": "0.8.*"
  }
}
